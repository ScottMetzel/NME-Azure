                    "apiVersion": "2020-08-01",
                    "name": "perfcounter1",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', parameters('workspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "LogicalDisk",
                        "instanceName": "C:",
                        "intervalSeconds": 60,
                        "counterName": "% Free Space"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter2",
