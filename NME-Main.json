{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "Region": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]",
            "metadata": {
                "description": "The name of the Azure region where all resources will be deployed."
            }
        },
        "DeploymentEnvironment": {
            "type": "string",
            "defaultValue": "Dev",
            "allowedValues": [
                "Dev",
                "Test",
                "Stage",
                "Prod"
            ],
            "metadata": {
                "description": "The deployment environment which Azure resources will be deployed in."
            }
        },
        "BaseName": {
            "type": "string",
            "minLength": 2,
            "maxLength": 10,
            "metadata": {
                "description": "This parameter is used as a base name for all Azure resources created during deployment, e.g., 'Dev-APP-BaseName-01'."
            }
        },
        "BaseSequenceNumber": {
            "type": "int",
            "defaultValue": 1,
            "minValue": 1,
            "maxValue": 99,
            "metadata": {
                "description": "The base sequence number for all resources to create."
            }
        },
        "azureTagPrefix": {
            "type": "string",
            "defaultValue": "NMW",
            "metadata": {
                "description": "Prefix for Azure Tags"
            }
        },
        "AppServicePlanSKU": {
            "type": "string",
            "defaultValue": "B3",
            "metadata": {
                "description": "The SKU of App Service Plan"
            }
        },
        "SQLServerUsername": {
            "type": "string",
            "minLength": 4,
            "metadata": {
                "description": "The username for the Azure SQL Server."
            }
        },
        "SQLServerPassword": {
            "type": "securestring",
            "minLength": 8,
            "metadata": {
                "description": "The password for the Azure SQL Server."
            }
        },
        "SQLCollation": {
            "type": "string",
            "allowedValues": [
                "SQL_Latin1_General_CP1_CI_AS"
            ],
            "defaultValue": "SQL_Latin1_General_CP1_CI_AS",
            "metadata": {
                "description": "The database collation to use"
            }
        },
        "SQLDatabaseMaxSize": {
            "type": "int",
            "defaultValue": 268435456000
        },
        "SQLDatabaseTier": {
            "type": "string",
            "defaultValue": "Standard"
        },
        "SQLDatabaseSKU": {
            "type": "string",
            "defaultValue": "S1"
        },
        "_artifactsLocation": {
            "type": "string",
            "metadata": {
                "description": "The base URI where artifacts required by this template are located. When the template is deployed using the accompanying scripts, a private location in the subscription will be used and this value will be automatically generated."
            },
            "defaultValue": ""
        },
        "_artifactsLocationSasToken": {
            "type": "securestring",
            "metadata": {
                "description": "The sasToken required to access _artifactsLocation.  When the template is deployed using the accompanying scripts, a sasToken will be automatically generated."
            },
            "defaultValue": ""
        }
    },
    "variables": {
        "deploymentResourceGroup": "[resourceGroup().name]",
        "subscriptionID": "[subscription().subscriptionId]",
        "deploymentName-PartnerCenter": "pid-8c1c30c0-3e0a-4655-9e05-51dea63a0e32-partnercenter",
        "baseSequenceNumberFormatted": "[format('{0:00}', parameters('BaseSequenceNumber'))]",
        "baseSequenceNumber02": "[add(parameters('BaseSequenceNumber'), 1)]",
        "baseSequenceNumber02Formatted": "[format('{0:00}', variables('baseSequenceNumber02'))]",
        "appInsightsName": "[concat(parameters('DeploymentEnvironment'),'-APPIN-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted'))]",
        "appServiceName": "[concat(parameters('DeploymentEnvironment'),'-APP-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted'))]",
        "appServiceAppPackageURI": "
        https: //catalogartifact.azureedge.net/publicartifacts/nerdio.nerdio_wvd_manager-3569ee61-5bd2-4f19-a0f1-4e3dc403f20e-nwm-plan/Artifacts/web-app/app.zip",
        "appServicePlanName": "[concat(parameters('DeploymentEnvironment'),'-APPSP-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted'))]",
        "azureADLoginURI": "[if(equals(environment().name, 'AzureUSGovernment'), 'https://login.microsoftonline.us/', 'https://login.microsoftonline.com/')]",
        "automationAccountName": "[concat(parameters('DeploymentEnvironment'),'-AA-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted'))]",
        "automationAccountName02": "[concat(parameters('DeploymentEnvironment'),'-AA-',parameters('BaseName'),'-',variables('baseSequenceNumber02Formatted'))]",
        "automationAccountRunbookUpdateRunAsScriptURI": "[uri(parameters('_artifactsLocation'), concat('scripts/nmw-update-run-as.ps1', parameters('_artifactsLocationSasToken')))]",
        "automationAccountProperties": {
            "runbooks": [
                {
                    "name": "nmwUpdateRunAs",
                    "url": "[variables('automationAccountRunbookUpdateRunAsScriptURI')]",
                    "version": "1.0.0.0",
                    "type": "PowerShell",
                    "description": "Update using automation Run As account"
                }
            ],
            "modules": {
                "profile": {
                    "name": "AzureRM.Profile",
                    "url": "https://www.powershellgallery.com/api/v2/package/AzureRM.profile/5.8.3"
                },
                "other": [
                    {
                        "name": "AzureAD",
                        "url": "https://www.powershellgallery.com/api/v2/package/AzureAD/2.0.2.76"
                    },
                    {
                        "name": "AzureRM.Resources",
                        "url": "https://www.powershellgallery.com/api/v2/package/AzureRM.Resources/6.7.3"
                    },
                    {
                        "name": "AzureRM.KeyVault",
                        "url": "https://www.powershellgallery.com/api/v2/package/AzureRM.KeyVault/5.2.1"
                    },
                    {
                        "name": "AzureRM.Automation",
                        "url": "https://www.powershellgallery.com/api/v2/package/AzureRM.Automation/6.1.1"
                    },
                    {
                        "name": "AzureRM.Compute",
                        "url": "https://www.powershellgallery.com/api/v2/package/AzureRM.Compute/5.9.1"
                    },
                    {
                        "name": "AzureRM.Websites",
                        "url": "https://www.powershellgallery.com/api/v2/package/AzureRM.Websites/5.2.0"
                    }
                ]
            }
        },
        "sqlServerName": "[concat(parameters('DeploymentEnvironment'),'-ASQL-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted'))]",
        "sqlServerSuffix": "[environment().suffixes.sqlServerHostname]",
        "sqlDatabaseName": "[toLower(concat(parameters('DeploymentEnvironment'),'-ASQLDB-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted')))]",
        "keyVaultName": "[concat(parameters('DeploymentEnvironment'),'-KV-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted'))]",
        "logAnalyticsWorkspaceName": "[concat(parameters('DeploymentEnvironment'),'-LAW-',parameters('BaseName'),'-',variables('baseSequenceNumberFormatted'))]"
    },
    "resources": [
        {
            "type": "Microsoft.Insights/components",
            "apiVersion": "2020-02-02-preview",
            "name": "[variables('appInsightsName')]",
            "location": "[parameters('Region')]",
            "tags": {
                "displayName": "AppInsightsComponent"
            },
            "kind": "web",
            "properties": {
                "Application_Type": "web"
            }
        },
        {
            "type": "Microsoft.Web/serverfarms",
            "apiVersion": "2019-08-01",
            "name": "[variables('appServicePlanName')]",
            "location": "[parameters('Region')]",
            "kind": "app",
            "properties": {},
            "sku": {
                "name": "[parameters('AppServicePlanSKU')]"
            }
        },
        {
            "type": "Microsoft.Web/sites",
            "apiVersion": "2019-08-01",
            "name": "[variables('appServiceName')]",
            "location": "[parameters('Region')]",
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms', variables('appServicePlanName'))]",
                "[resourceId('microsoft.insights/components/', variables('appInsightsName'))]"
            ],
            "kind": "app",
            "identity": {
                "type": "SystemAssigned"
            },
            "properties": {
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', variables('appServicePlanName'))]",
                "siteConfig": {
                    "AlwaysOn": true,
                    "use32BitWorkerProcess": false,
                    "appSettings": [
                        {
                            "name": "AzureAd:Instance",
                            "value": "[variables('azureADLoginURI')]"
                        },
                        {
                            "name": "Deployment:AzureType",
                            "value": "[environment().name]"
                        },
                        {
                            "name": "Deployment:Region",
                            "value": "[parameters('Region')]"
                        },
                        {
                            "name": "Deployment:KeyVaultName",
                            "value": "[variables('keyVaultName')]"
                        },
                        {
                            "name": "Deployment:SubscriptionId",
                            "value": "[subscription().subscriptionId]"
                        },
                        {
                            "name": "Deployment:SubscriptionDisplayName",
                            "value": "[subscription().displayName]"
                        },
                        {
                            "name": "Deployment:TenantId",
                            "value": "[subscription().tenantId]"
                        },
                        {
                            "name": "Deployment:ResourceGroupName",
                            "value": "[resourceGroup().name]"
                        },
                        {
                            "name": "Deployment:WebAppName",
                            "value": "[variables('appServiceName')]"
                        },
                        {
                            "name": "Deployment:AutomationAccountName",
                            "value": "[variables('automationAccountName')]"
                        },
                        {
                            "name": "Deployment:AutomationEnabled",
                            "value": "True"
                        },
                        {
                            "name": "Deployment:AzureTagPrefix",
                            "value": "[parameters('azureTagPrefix')]"
                        },
                        {
                            "name": "Deployment:UpdaterRunbookRunAs",
                            "value": "nmwUpdateRunAs"
                        },
                        {
                            "name": "Deployment:LogAnalyticsWorkspace",
                            "value": "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                        },
                        {
                            "name": "Deployment:ScriptedActionAccount",
                            "value": "[resourceId('Microsoft.Automation/automationAccounts', variables('automationAccountName02'))]"
                        },
                        {
                            "name": "ApplicationInsights:InstrumentationKey",
                            "value": "[reference(resourceId('Microsoft.Insights/components', variables('appInsightsName'))).InstrumentationKey]"
                        },
                        {
                            "name": "ApplicationInsights:ConnectionString",
                            "value": "[reference(resourceId('Microsoft.Insights/components', variables('appInsightsName'))).ConnectionString]"
                        }
                    ]
                }
            },
            "resources": [
                {
                    "type": "extensions",
                    "apiVersion": "2019-08-01",
                    "name": "MSDeploy",
                    "location": "[parameters('Region')]",
                    "dependsOn": [
                        "[resourceId('Microsoft.Web/sites', variables('appServiceName'))]"
                    ],
                    "properties": {
                        "packageUri": "[variables('appServiceAppPackageURI')]"
                    }
                }
            ]
        },
        {
            "type": "Microsoft.Automation/automationAccounts",
            "apiVersion": "2020-01-13-preview",
            "name": "[variables('automationAccountName')]",
            "location": "[parameters('Region')]",
            "properties": {
                "sku": {
                    "name": "Basic"
                }
            },
            "resources": [
                {
                    "type": "variables",
                    "apiVersion": "2020-01-13-preview",
                    "name": "subscriptionId",
                    "dependsOn": [
                        "[resourceId('Microsoft.Automation/automationAccounts', variables('automationAccountName'))]"
                    ],
                    "properties": {
                        "isEncrypted": false,
                        "description": "Azure Subscription Id",
                        "value": "[concat('\"', variables('subscriptionID'), '\"')]"
                    }
                },
                {
                    "type": "variables",
                    "apiVersion": "2020-01-13-preview",
                    "name": "webAppName",
                    "dependsOn": [
                        "[resourceId('Microsoft.Automation/automationAccounts', variables('automationAccountName'))]"
                    ],
                    "properties": {
                        "isEncrypted": false,
                        "description": "Web App Name",
                        "value": "[concat('\"', variables('appServiceName'), '\"')]"
                    }
                },
                {
                    "type": "variables",
                    "apiVersion": "2020-01-13-preview",
                    "name": "resourceGroupName",
                    "dependsOn": [
                        "[resourceId('Microsoft.Automation/automationAccounts', variables('automationAccountName'))]"
                    ],
                    "properties": {
                        "isEncrypted": false,
                        "description": "Resource group",
                        "value": "[concat('\"', variables('deploymentResourceGroup'), '\"')]"
                    }
                }
            ]
        },
        {
            "type": "Microsoft.Automation/automationAccounts/runbooks",
            "apiVersion": "2019-06-01",
            "name": "[concat(variables('automationAccountName'), '/', variables('automationAccountProperties').runbooks[copyIndex()].name)]",
            "location": "[parameters('Region')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('automationAccountName'))]"
            ],
            "properties": {
                "description": "[variables('automationAccountProperties').runbooks[copyIndex()].description]",
                "runbookType": "[variables('automationAccountProperties').runbooks[copyIndex()].type]",
                "logProgress": false,
                "logVerbose": true,
                "publishContentLink": {
                    "uri": "[variables('automationAccountProperties').runbooks[copyIndex()].url]",
                    "version": "[variables('automationAccountProperties').runbooks[copyIndex()].version]"
                }
            },
            "copy": {
                "name": "runbooksLoop",
                "count": "[length(variables('automationAccountProperties').runbooks)]"
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('automationAccountName'), '/', variables('automationAccountProperties').modules.profile.name)]",
            "location": "[parameters('Region')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('automationAccountName'))]"
            ],
            "properties": {
                "contentLink": {
                    "uri": "[variables('automationAccountProperties').modules.profile.url]"
                }
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts/modules",
            "apiVersion": "2015-10-31",
            "name": "[concat(variables('automationAccountName'), '/', variables('automationAccountProperties').modules.other[copyIndex()].name)]",
            "location": "[parameters('Region')]",
            "dependsOn": [
                "[resourceId('Microsoft.Automation/automationAccounts', variables('automationAccountName'))]",
                "[resourceId('Microsoft.Automation/automationAccounts/modules', variables('automationAccountName'), variables('automationAccountProperties').modules.profile.name)]"
            ],
            "copy": {
                "name": "modulesLoop",
                "count": "[length(variables('automationAccountProperties').modules.other)]",
                "mode": "Serial"
            },
            "properties": {
                "contentLink": {
                    "uri": "[variables('automationAccountProperties').modules.other[copyIndex()].url]"
                }
            }
        },
        {
            "type": "Microsoft.Automation/automationAccounts",
            "apiVersion": "2020-01-13-preview",
            "name": "[variables('automationAccountName02')]",
            "location": "[parameters('Region')]",
            "properties": {
                "sku": {
                    "name": "Basic"
                }
            }
        },
        {
            "type": "Microsoft.KeyVault/vaults",
            "apiVersion": "2019-09-01",
            "name": "[variables('keyVaultName')]",
            "location": "[parameters('Region')]",
            "dependsOn": [
                "[resourceId('Microsoft.Web/sites', variables('appServiceName'))]"
            ],
            "properties": {
                "sku": {
                    "family": "A",
                    "name": "Standard"
                },
                "tenantId": "[reference(resourceId('Microsoft.Web/sites/', variables('appServiceName')), '2019-08-01', 'Full').identity.tenantId]",
                "accessPolicies": [
                    {
                        "tenantId": "[reference(resourceId('Microsoft.Web/sites/', variables('appServiceName')), '2019-08-01', 'Full').identity.tenantId]",
                        "objectId": "[reference(resourceId('Microsoft.Web/sites/', variables('appServiceName')), '2019-08-01', 'Full').identity.principalId]",
                        "permissions": {
                            "secrets": [
                                "get",
                                "list",
                                "set",
                                "delete"
                            ]
                        }
                    }
                ],
                "enabledForDeployment": false
            },
            "resources": [
                {
                    "type": "Microsoft.KeyVault/vaults/secrets",
                    "apiVersion": "2019-09-01",
                    "name": "[concat(variables('keyVaultName'), '/ConnectionStrings--DefaultConnection')]",
                    "dependsOn": [
                        "[resourceId('Microsoft.KeyVault/vaults', variables('keyVaultName'))]"
                    ],
                    "properties": {
                        "value": "[concat('Server=tcp:', variables('sqlServerName'), variables('sqlServerSuffix'), ',1433;Initial Catalog=', variables('sqlDatabaseName'), ';Persist Security Info=False;User ID=', parameters('SQLServerUsername'), ';Password=', parameters('SQLServerPassword'), ';MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;')]",
                        "attributes": {
                            "enabled": true
                        }
                    }
                }
            ]
        },
        {
            "type": "Microsoft.OperationalInsights/workspaces",
            "apiVersion": "2020-08-01",
            "name": "[variables('logAnalyticsWorkspaceName')]",
            "location": "[parameters('Region')]",
            "tags": {
                "NMW_OBJECT_TYPE": "LOG_ANALYTICS_WORKSPACE"
            },
            "properties": {},
            "resources": [
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "SystemEvents",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsEvent",
                    "properties": {
                        "eventLogName": "System",
                        "eventTypes": [
                            {
                                "eventType": "Error"
                            },
                            {
                                "eventType": "Warning"
                            }
                        ]
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "ApplicationEvents",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsEvent",
                    "properties": {
                        "eventLogName": "Application",
                        "eventTypes": [
                            {
                                "eventType": "Error"
                            },
                            {
                                "eventType": "Warning"
                            }
                        ]
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "TerminalServicesLocalSessionManagerOperational",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsEvent",
                    "properties": {
                        "eventLogName": "Microsoft-Windows-TerminalServices-LocalSessionManager/Operational",
                        "eventTypes": [
                            {
                                "eventType": "Error"
                            },
                            {
                                "eventType": "Warning"
                            },
                            {
                                "eventType": "Information"
                            }
                        ]
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "TerminalServicesRemoteConnectionManagerAdmin",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsEvent",
                    "properties": {
                        "eventLogName": "Microsoft-Windows-TerminalServices-RemoteConnectionManager/Admin",
                        "eventTypes": [
                            {
                                "eventType": "Error"
                            },
                            {
                                "eventType": "Warning"
                            },
                            {
                                "eventType": "Information"
                            }
                        ]
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "MicrosoftFSLogixAppsOperational",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsEvent",
                    "properties": {
                        "eventLogName": "Microsoft-FSLogix-Apps/Operational",
                        "eventTypes": [
                            {
                                "eventType": "Error"
                            },
                            {
                                "eventType": "Warning"
                            },
                            {
                                "eventType": "Information"
                            }
                        ]
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "MicrosoftFSLogixAppsAdmin",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsEvent",
                    "properties": {
                        "eventLogName": "Microsoft-FSLogix-Apps/Admin",
                        "eventTypes": [
                            {
                                "eventType": "Error"
                            },
                            {
                                "eventType": "Warning"
                            },
                            {
                                "eventType": "Information"
                            }
                        ]
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter1",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "LogicalDisk",
                        "instanceName": "C:",
                        "intervalSeconds": 60,
                        "counterName": "% Free Space"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter2",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "LogicalDisk",
                        "instanceName": "C:",
                        "intervalSeconds": 30,
                        "counterName": "Avg. Disk Queue Length"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter3",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "LogicalDisk",
                        "instanceName": "C:",
                        "intervalSeconds": 60,
                        "counterName": "Avg. Disk sec/Transfer"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter4",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "LogicalDisk",
                        "instanceName": "C:",
                        "intervalSeconds": 30,
                        "counterName": "Current Disk Queue Length"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter5",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "Memory",
                        "instanceName": "*",
                        "intervalSeconds": 30,
                        "counterName": "Available Mbytes"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter6",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "Memory",
                        "instanceName": "*",
                        "intervalSeconds": 30,
                        "counterName": "Page Faults/sec"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter7",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "Memory",
                        "instanceName": "*",
                        "intervalSeconds": 30,
                        "counterName": "Pages/sec"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter8",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "Memory",
                        "instanceName": "*",
                        "intervalSeconds": 30,
                        "counterName": "% Committed Bytes In Use"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
                    "name": "perfcounter9",
                    "dependsOn": [
                        "[resourceId('Microsoft.OperationalInsights/workspaces', variables('logAnalyticsWorkspaceName'))]"
                    ],
                    "kind": "WindowsPerformanceCounter",
                    "properties": {
                        "objectName": "PhysicalDisk",
                        "instanceName": "*",
                        "intervalSeconds": 30,
                        "counterName": "Avg. Disk Queue Length"
                    }
                },
                {
                    "type": "datasources",
                    "apiVersion": "2020-08-01",
